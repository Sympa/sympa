		< $(srcdir)/$@.in > $@

install-data-hook:
	cd  $(DESTDIR)$(defaultdir)/scenari; \
	for file in $(DEFAULT_SCENARIOS); do \
		ln -sf $$file `echo $$file | sed -e 's/\.[^.]*$$/.default/'`; \
	done
