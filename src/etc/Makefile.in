	test -z "$(defaultdir)" || $(mkdir_p) "$(DESTDIR)$(defaultdir)"
	@$(am__vpath_adj_setup) \
	list='$(nobase_default_DATA)'; for p in $$list; do \
	  if test -f "$$p"; then d=; else d="$(srcdir)/"; fi; \
	  $(am__vpath_adj) \
	  echo " $(nobase_defaultDATA_INSTALL) '$$d$$p' '$(DESTDIR)$(defaultdir)/$$f'"; \
	  $(nobase_defaultDATA_INSTALL) "$$d$$p" "$(DESTDIR)$(defaultdir)/$$f"; \
	done

